{% extends 'StasmoPanaxBundle::layout.html.twig' %}

{% block body %}

<ul class="nav nav-pills">
  <li class="active"><a href="{{ url('music_new') }}">New Music</a></li>
</ul>
<table class="table">
{% for music in musics %}
<tr>
	<td>{{ music.name }}</td>
	<td>{{ music.embedString|e }}</td>
	<td>{% if music.displayLeft %}left{% else %}right{% endif %}</td>
	<td>Display order: {{ music.displayOrder }}</td>
	<td><a href="/music/edit/{{ music.id }}">Edit</a></td>
	<td><a href="/music/delete/{{ music.id }}">Delete</a></td>
</tr>
{% endfor %}
</table>
{% endblock %}